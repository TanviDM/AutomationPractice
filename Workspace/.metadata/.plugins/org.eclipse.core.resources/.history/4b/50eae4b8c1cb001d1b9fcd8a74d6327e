package day1.basicprogram;

public class PrimeNumber {



		public static void main(String[] args) {
			// TODO Auto-generated method stub
			
			
			int i,n=100;
			for(i=2;i<n;i++)
				if(n%i==0)
					System.out.println( " Prime");            
	            else            	
	               System.out.println(  " Not a Prime");            
			
			
		}
	}



