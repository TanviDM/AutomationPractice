package ForLoop;

public class PrimeNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for(int i= 1 ;i<=100 ;i++) {
			if(100%i==0) {
				System.out.println("Prime number" + i);
			}
			else
			{
				System.out.println(" Not Prime number" + i);
			}
			
			
		}

	}

}
